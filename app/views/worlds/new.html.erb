<section class="wrapper">
<header class="main-header">
  <h2>New World</h2>
</header>

<main class="content">
  <%= form_for @world do |f| %>
    <div class="form_meta_field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>

    <div class="form_meta_field">
      <%= f.label :img_url %>
      <%= f.text_field :img_url %>
    </div>

    <div class="form_meta_field">
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>

    <%= f.submit %>
  <% end %>
</main>
</section>
